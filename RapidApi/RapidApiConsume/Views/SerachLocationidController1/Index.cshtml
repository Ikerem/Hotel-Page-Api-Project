@model List<BookingApiLocationSerchViewvModel>
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
<div class="container">
    <br />
    @using (Html.BeginForm("Index", "SerachLocationidController1",FormMethod.Post))
    {
        <label>Şehir Adını Giriniz</label>
        <input type="text" class="form-control" name="cityname" />
        <br />
        <button class="btn btn-primary">Şehir İçin id bul</button>
    }

    <br />
    <br />
    <table class="table table-bordered">
        <tr>
         
            <th>Lokasyon</th>
          

        </tr>
        @foreach (var item in Model)
        {
          
            <tr>
                <td>@item.dest_id</td>
           

            </tr>
        }
    </table>
    <br />
    <a href="/BookingByCity/Index/" class=" btn btn-info"> Yeni Otel Rezervasyon  Bilgisi</a>
</div>