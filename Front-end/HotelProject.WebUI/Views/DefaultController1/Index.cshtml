
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_HeadPartial")

<body>
    <div class="container-fluid  p-0">
        <!-- Spinner Start -->
        @await Component.InvokeAsync("_SpinnerPartial")
        <!-- Spinner End -->
        <!-- Header Start -->
        @await Component.InvokeAsync("_NavbarPartial")
        <!-- Header End -->
        <!-- Carousel Start -->
        @await Component.InvokeAsync("_SliderPartial")
        <!-- Carousel End -->
        <!-- Booking Start -->
        @*@await Component.InvokeAsync("_RezervationPartial")*@
        <!-- Booking End -->
        <!-- About Start -->
        @await Component.InvokeAsync("_AbaoutUsPartial")
        <!-- About End -->
        <!-- Room Start -->
        @await Component.InvokeAsync("_RoomPartial")
        <!-- Room End -->
        <!-- Video Start -->
        @await Component.InvokeAsync("_TrailerPartial")
        <!-- Video End -->
        <!-- Service Start -->
        @await Component.InvokeAsync("_ServicesPartial")
        <!-- Service End -->
        <!-- Testimonial Start -->
        @await Component.InvokeAsync("_TestimonialPartial")
        <!-- Testimonial End -->
        <!-- Team Staff Start -->
        @await Component.InvokeAsync("_StaffPartial")
        <!-- Team End -->
        <!-- Newsletter Start -->
        @await  Html.PartialAsync("NewsLatterResult")
        <!-- Newsletter Start -->
        <!-- Footer Start -->
        @await Component.InvokeAsync("_FooterPartial")
        <!-- Footer End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    @await Component.InvokeAsync("_JavaScriptPartial")
</body>

</html>
