@model List<HotelProject.EntityLayer.Concrete.AppUser>


@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayoutController1/_AdmniLayout.cshtml";
}


<table class="table table-xs mb-0">
    <thead>
        <tr>
          
            <th>Ad-Soyad</th>
            <th>Şehir</th>
            <th>Durum</th>
     
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
        <tr>
       
                <td>@item.Name @item.SurName</td>
                <td>@item.Sehir</td>
            <td><i class="fa fa-circle-o text-success  mr-2"></i> Aktif</td>
            

        </tr>
  
    }
    </tbody>
</table>
<a href="" class=" btn btn-outline-info">Yeni Personel</a>

