@model UpdateServiceDto
@{
    ViewData["Title"] = "updateService";
    Layout = "~/Views/AdminLayoutController1/_AdmniLayout.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="Serviceid"  />
    <label> Hizmet Başlığı</label>
    <input type="text" asp-for="Title" class="form-control" />
    <span asp-validation-for="Title" class="text-info"></span>
    <br />
    <label> Hizmet İcon</label>
    <input type="text" asp-for="Serviceicon" class="form-control" />
    <span asp-validation-for="Serviceicon" class="text-info"></span>
    <br />
    <label> Açıklama</label>
    <input type="text" asp-for="Description" class="form-control" />
    <span asp-validation-for="Description" class="text-info"></span>
    <br />

    <button class=" btn btn-outline-success">Güncelle</button>
</form>

