@model List<ResultAboutDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayoutController1/_AdmniLayout.cshtml";
}
<table class="table table-bordered">

    <tr>
        <th>ID</th>
        <th>Başlık 1</th>
 
        
  
        <th>Güncelle</th>
      

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.Aboutid</th>
            <th>@item.Description</th>

            <td> <a href="/AdminAboutController1/UpdateAbout/@item.Aboutid" class="btn btn-danger">Güncelle</a>  </td>
          
        

        </tr>

    }
</table>


